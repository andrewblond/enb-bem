    "lodash": "4.11.1",
    "vow": "0.4.12"
  },
  "devDependencies": {
    "enb": ">=0.15.0 <2.0.0",
    "gulp-eval": "1.0.0-0",
    "istanbul": "0.4.2",
    "jscs": "2.8.0",
    "jshint": "2.9.1",
    "mocha": "2.3.4",
    "mock-enb": "0.3.2",
    "mock-fs": "3.8.0",
    "must": "0.13.1",
    "proxyquire": "1.7.4",
    "seedrandom": "2.4.2"
