  },
  "dependencies": {
    "bem-naming": "1.0.1",
    "bem-walk": "1.0.0-1",
    "clear-require": "1.0.1",
    "enb-async-require": "1.0.1",
    "enb-require-or-eval": "1.0.2",
    "inherit": "2.2.3",
